    <style>
      .box {
        /* width: 210mm; */
        /* height: 297mm; */
        /* background-color: rgb(255, 255, 187); */
        margin: auto;
        padding: 5rem;
    }

      table, th, td {
        border: 1px solid black;
        border-collapse: collapse;
        vertical-align: "center";
      }
      td {
          text-align: center;
        }
      .l-td {
          text-align: inherit;
        }
      .n-b {
        border: none;
      }
      .t-l {
        text-align: left;
      }
      .f-b {
        font-weight: 700;
      }
    </style>
  </head>
  <body>
    <% var dir = process.cwd() %>
    <page class="1">
      <div class="box">
        <%- include(dir + '/views/partials/page1'); %>
      </div>
    </page>

    <page class="2">
      <div class="box">
        <%- include(dir + '/views/partials/page2');  %>
      </div>
    </page>

    <page class="3">
