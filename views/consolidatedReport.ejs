<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
      integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
      crossorigin="anonymous"
    />
    <style>
      body {
        zoom: .7;
      }
      @page {
       size: 7in 9.25in;
       margin: 27mm 16mm 27mm 16mm;
      }
      .box {
        width: 210mm !important;
        height: 297mm !important;
        page-break-inside: auto;
        /* background-color: rgb(255, 255, 187); */
        /* margin: auto; */
        /* padding: 5rem; */
        padding-left: 3rem;
      }
      table, th, td {
        page-break-inside:auto;
        border: 1px solid black;
        border-collapse: collapse;
        vertical-align: "center";
      }
      tr {
        page-break-inside:avoid;
         page-break-after:auto 
      }
      td {
          text-align: center;
        }
      .l-td {
          padding-left: 1rem;
          text-align: inherit;
        }
      .n-b {
        border: none;
      }
      .t-l {
        padding-left: 0.5rem;
        text-align: left;
      }
      .f-b {
        font-weight: 700;
      }
      .t-c{
        text-align: center;
      }
      .align-center{
        display: flex;
        flex-direction: row;
        align-items: center;
      }
    </style>
  </head>
  <body>
    <% var dir = process.cwd() %>
    <page class="1">
      <div class="box" style="margin-top: 8rem;">
        <%- include(dir + '/views/partials/page1'); %>
      </div>
    </page>

    <page class="2">
      <div class="box" style="padding-top: 2rem;">
        <%- include(dir + '/views/partials/page2');  %>
      </div>
    </page>

    <page class="3">
      <div class="box" style="padding-top: 4rem;">
      <%- include(dir + '/views/partials/page3');  %>
      </div>
    </page>

    <page class="4">
      <div class="box" style="padding-top: 8rem;">
      <%- include(dir + '/views/partials/page4');  %>
      </div>
    </page>
     <page class="5">
      <div class="box" style="padding-top: 8rem;">
      <%- include(dir + '/views/partials/page5'); %>
      </div>
    </page>
     <page class="6">
      <div class="box">
      <%- include(dir + '/views/partials/page6');  %>
      </div>
    </page>
     <page class="7">
      <div class="box" style="padding-top: 5rem;">
      <%- include(dir + '/views/partials/page7');   %>
      </div>
    </page>
  </body>
   <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
  </body>
</html>
